super + Return
	st 
super + d
	dmenu_run 
super + e
	emojis
super + shift + Tab
	lockme
super + shift + a
	appender
super + Insert
	bash -e passmenu
super + shift + Insert
	bash -e qrpass
super + shift + b
	bookmarks
super + End
    /home/maxwell/bin/interactive_bookmarks.sh

super + shift + Escape
	set_kb_map && notify-send -t 1000 "Correctly Set Capslock => Escape üòç"
super + shift + w
    $bin/wg_switcher.sh
super + w
    wallpaper-shader
super + shift + t
	faucet
super + shift + s
    screenshot
super + shift + q
    as_qr
super + shift + o
    xdotool mousemove 2200 1000 && sleep 0.1 && xdotool mousemove 2250 1050 && sleep 0.1 && xdotool mousemove 2300 && 1000

super + shift + m
    mp_queue_song


