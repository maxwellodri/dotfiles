super + Return
	st 
super + d
	dmenu_run 
super + e
	emojis
super + shift + Tab
	lockme
super + shift + a
	art.sh
super + Insert
	bash -e passmenu
super + shift + b
	bookmarks
super + End
    /home/maxwell/bin/interactive_bookmarks.sh

super + shift + q
	set_kb_map && notify-send -t 1000 "Correctly Set Capslock => Escape üòç"
super + shift + w
	togglerc ~/vimwiki/index.wiki
super + w
    wallpaper-shader
super + shift + t
	torr
super + shift + s
    setsid ffplay -nodisp -autoexit $dotfiles/media/camera.ogg && screendump && sleep 0.2 && notify-send -t 1000 "Screenshot Taken üì∏"
super + shift + o
    xdotool mousemove 2200 1000 && sleep 0.1 && xdotool mousemove 2250 1050 && sleep 0.1 && xdotool mousemove 2300 && 1000


